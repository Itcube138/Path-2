<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <HorizontalScrollView
        android:layout_width="409dp"
        android:layout_height="71dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="88dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="SpeakableTextPresentCheck">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent">

            <Button
                android:id="@+id/popular"
                android:layout_width="150dp"
                android:layout_height="71dp"
                android:layout_weight="1"
                android:backgroundTint="#1A6FED"
                android:text="Популярное"
                android:textColor="#FEFEFE" />

            <View
                android:id="@+id/divider"
                android:layout_width="11dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="?android:attr/listDivider"
                android:visibility="invisible" />

            <Button
                android:id="@+id/covid"
                android:layout_width="155dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:backgroundTint="#FFF4F4F8"
                android:text="Covid"
                android:textColor="#989898" />

            <View
                android:id="@+id/divider2"
                android:layout_width="11dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="?android:attr/listDivider"
                android:visibility="invisible" />

            <Button
                android:id="@+id/complecs"
                android:layout_width="183dp"
                android:layout_height="71dp"
                android:layout_weight="1"
                android:backgroundTint="#FFF4F4F8"
                android:text="Комплексные"
                android:textColor="#989898" />

        </LinearLayout>
    </HorizontalScrollView>

    <ScrollView
        android:layout_width="374dp"
        android:layout_height="410dp"
        android:layout_marginTop="160dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.486"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="SpeakableTextPresentCheck">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <com.google.android.material.appbar.MaterialToolbar
                android:id="@+id/materialToolbar11"
                android:layout_width="367dp"
                android:layout_height="172dp"
                android:layout_marginStart="2dp"
                android:layout_marginTop="192dp"
                android:layout_marginEnd="1dp"
                android:background="?attr/colorPrimary"
                android:minHeight="?attr/actionBarSize"
                android:theme="?attr/actionBarTheme"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <com.google.android.material.appbar.MaterialToolbar
                android:id="@+id/materialToolbar10"
                android:layout_width="367dp"
                android:layout_height="172dp"
                android:layout_marginStart="1dp"
                android:layout_marginTop="3dp"
                android:layout_marginEnd="3dp"
                android:background="?attr/colorPrimary"
                android:minHeight="?attr/actionBarSize"
                android:theme="?attr/actionBarTheme"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <Button
                android:id="@+id/dob1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="12dp"
                android:backgroundTint="#1A6FED"
                android:onClick="dob1"
                android:text="добавить"
                android:textColor="#FFFFFF"
                app:layout_constraintBottom_toBottomOf="@+id/materialToolbar10"
                app:layout_constraintEnd_toEndOf="@+id/materialToolbar10"
                app:layout_constraintTop_toTopOf="@+id/materialToolbar10"
                app:layout_constraintVertical_bias="0.516" />

            <Button
                android:id="@+id/dob2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="12dp"
                android:backgroundTint="#1A6FED"
                android:onClick="dob2"
                android:text="Добавить"
                android:textColor="#FFFFFF"
                app:layout_constraintBottom_toBottomOf="@+id/materialToolbar11"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="@+id/materialToolbar11"
                app:layout_constraintVertical_bias="0.508" />

            <TextView
                android:id="@+id/textView14"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:layout_marginTop="156dp"
                android:text="1800 ₽ "
                android:textColor="#000000"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/textView15"
                android:layout_width="53dp"
                android:layout_height="21dp"
                android:layout_marginStart="16dp"
                android:layout_marginTop="67dp"
                android:text="2 дня"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/textView18"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:layout_marginTop="345dp"
                android:text="690 ₽"
                android:textColor="#000000"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/textView19"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:layout_marginTop="270dp"
                android:text="1 день"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/textView20"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="3dp"
                android:layout_marginTop="4dp"
                android:text="ПЦР-тест на определение РНК коронавируса стандартный"
                android:textColor="#000000"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/textView21"
                android:layout_width="368dp"
                android:layout_height="32dp"
                android:layout_marginStart="3dp"
                android:layout_marginTop="195dp"
                android:text="Клинический анализ крови с лейкоцитарной формулой"
                android:textColor="#000000"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

        </androidx.constraintlayout.widget.ConstraintLayout>
    </ScrollView>

    <SearchView
        android:layout_width="361dp"
        android:layout_height="39dp"
        android:layout_marginStart="3dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="2dp"
        android:background="#ECECEC"
        android:contextClickable="false"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/analiz3"
        android:layout_width="74dp"
        android:layout_height="70dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="640dp"
        android:onClick="analiz"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/_enable"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageView
        android:id="@+id/rezult3"
        android:layout_width="101dp"
        android:layout_height="73dp"
        android:layout_marginStart="80dp"
        android:layout_marginTop="640dp"
        android:onClick="rezult"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/_2"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageView
        android:id="@+id/prof3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="288dp"
        android:layout_marginTop="636dp"
        android:onClick="prof"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/_1221"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageView
        android:id="@+id/podderg3"
        android:layout_width="95dp"
        android:layout_height="91dp"
        android:layout_marginStart="192dp"
        android:layout_marginTop="636dp"
        android:onClick="podderg"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/_21"
        tools:ignore="SpeakableTextPresentCheck" />

    <Button
        android:id="@+id/korzin"
        android:layout_width="378dp"
        android:layout_height="57dp"
        android:layout_marginStart="161dp"
        android:layout_marginTop="575dp"
        android:layout_marginEnd="162dp"
        android:backgroundTint="#1A6FED"
        android:onClick="korzin"
        android:text="В корзину                     2490 ₽"
        android:textColor="#E2ECFC"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>