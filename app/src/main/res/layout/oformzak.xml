<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageButton
        android:id="@+id/back"
        android:layout_width="68dp"
        android:layout_height="75dp"
        android:layout_marginStart="6dp"
        android:backgroundTint="#00FFFFFF"
        android:onClick="Back1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/bg"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="18dp"
        android:layout_height="18dp"
        android:layout_marginStart="31dp"
        android:layout_marginTop="27dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/vector" />

    <ScrollView
        android:layout_width="409dp"
        android:layout_height="654dp"
        android:layout_marginStart="1dp"
        android:layout_marginTop="76dp"
        android:visibility="visible"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <TextView
                android:id="@+id/textView16"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Оформление заказа"
                android:textColor="#000000"
                android:textSize="24dp"
                tools:layout_editor_absoluteX="31dp"
                tools:layout_editor_absoluteY="57dp" />

            <View
                android:id="@+id/divider3"
                android:layout_width="match_parent"
                android:layout_height="17dp"
                android:background="?android:attr/listDivider"
                android:visibility="invisible" />

            <TextView
                android:id="@+id/textView23"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Адрес*" />

            <EditText
                android:id="@+id/addres"
                android:layout_width="match_parent"
                android:layout_height="52dp"
                android:background="#F4F4F8"
                android:backgroundTint="#F4F4F8"
                android:ems="10"
                android:enabled="true"
                android:hint="Введите ваш адрес"
                android:inputType="textPersonName"
                android:onClick="addres" />

            <View
                android:id="@+id/divider4"
                android:layout_width="match_parent"
                android:layout_height="17dp"
                android:background="?android:attr/listDivider"
                android:visibility="invisible" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Дата и время*" />

            <EditText
                android:id="@+id/data"
                android:layout_width="match_parent"
                android:layout_height="56dp"
                android:background="#F4F4F8"
                android:backgroundTint="#F4F4F8"
                android:ems="10"
                android:enabled="true"
                android:hint="Выберите дату и время"
                android:inputType="textPersonName"
                android:onClick="data" />

            <View
                android:id="@+id/divider5"
                android:layout_width="match_parent"
                android:layout_height="37dp"
                android:background="?android:attr/listDivider"
                android:visibility="invisible" />

            <TextView
                android:id="@+id/textView26"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Кто будет сдавать анализы?*" />

            <EditText
                android:id="@+id/pacient"
                android:layout_width="match_parent"
                android:layout_height="59dp"
                android:background="#F4F4F8"
                android:backgroundTint="#F4F4F8"
                android:ems="10"
                android:enabled="true"
                android:hint="Выберите пациента"
                android:inputType="textPersonName"
                android:onClick="pacient" />

            <View
                android:id="@+id/divider6"
                android:layout_width="match_parent"
                android:layout_height="10dp"
                android:background="?android:attr/listDivider"
                android:visibility="invisible" />

            <Button
                android:id="@+id/pacient2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:backgroundTint="#FEFEFE"
                android:onClick="pacient2"
                android:text="Добавить ещё пациента"
                android:textColor="#3780EF" />

            <View
                android:id="@+id/divider7"
                android:layout_width="match_parent"
                android:layout_height="31dp"
                android:background="?android:attr/listDivider"
                android:visibility="invisible" />

            <TextView
                android:id="@+id/textView27"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Телефон*" />

            <EditText
                android:id="@+id/editTextTextPersonName8"
                android:layout_width="match_parent"
                android:layout_height="62dp"
                android:background="#F4F4F8"
                android:backgroundTint="#F4F4F8"
                android:ems="10"
                android:hint="Введите свой номер телефона"
                android:inputType="textPersonName" />

            <TextView
                android:id="@+id/textView28"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Комментарий" />

            <View
                android:id="@+id/divider8"
                android:layout_width="match_parent"
                android:layout_height="17dp"
                android:background="?android:attr/listDivider"
                android:visibility="invisible" />

            <EditText
                android:id="@+id/editTextTextMultiLine"
                android:layout_width="match_parent"
                android:layout_height="208dp"
                android:background="#F4F4F8"
                android:backgroundTint="#F4F4F8"
                android:ems="10"
                android:gravity="start|top"
                android:hint="Можете оставить свои пожелания"
                android:inputType="textMultiLine"
                android:textSize="15sp" />

            <View
                android:id="@+id/divider9"
                android:layout_width="match_parent"
                android:layout_height="31dp"
                android:background="?android:attr/listDivider"
                android:visibility="invisible" />

            <TextView
                android:id="@+id/textView29"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="#F4F4F8"
                android:backgroundTint="#F4F4F8"
                android:text="Оплата Apple Pay"
                android:textColor="#000000" />

            <View
                android:id="@+id/divider10"
                android:layout_width="match_parent"
                android:layout_height="17dp"
                android:background="#F4F4F8"
                android:backgroundTint="#F4F4F8"
                android:visibility="visible" />

            <TextView
                android:id="@+id/textView30"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="#F4F4F8"
                android:backgroundTint="#F4F4F8"
                android:text="Промокод" />

            <View
                android:id="@+id/divider11"
                android:layout_width="match_parent"
                android:layout_height="31dp"
                android:background="#F4F4F8"
                android:backgroundTint="#F4F4F8"
                android:visibility="visible" />

            <TextView
                android:id="@+id/textView32"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="#F4F4F8"
                android:backgroundTint="#F4F4F8"
                android:text="1 анализ                                                                          690 ₽"
                android:textColor="#000000" />

            <Button
                android:id="@+id/zakazat"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:backgroundTint="#1A6FED"
                android:onClick="zakazat"
                android:text="Заказать"
                android:textColor="#FFFFFF" />

            <View
                android:id="@+id/divider12"
                android:layout_width="match_parent"
                android:layout_height="103dp"
                android:background="?android:attr/listDivider"
                android:visibility="invisible" />

            <ImageButton
                android:id="@+id/imageButton"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:srcCompat="@drawable/icon3s" />

            <Button
                android:id="@+id/button11"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Button" />
        </LinearLayout>
    </ScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>